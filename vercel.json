{
  "builds": [
    {
      "src": "smart_house_backend/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "smart_house_backend/wsgi.py"
    }
  ],
  "env": {
    "DEBUG": "False",
    "SECRET_KEY": "@django_secret_key",
    "ALLOWED_HOSTS": ".vercel.app",
    "DATABASE_URL": "@database_url"
  }
}